<%- include header %>
<section id="main">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="well">
                    <div class="container">
                        <h1>標題</h1>
                        <hr>
                        <p>請完成以下題目</p>
                        <p class="text-danger">*必填</p>
                        <br>
                    </div>
                    <form>
                        <fieldset>
                            <div class="col-md-8 col-md-offset-0">
                                <strong>
                                    <h4>1. 你的名字</h4>
                                </strong>
                                <input type="text" class="form-control" placeholder="你的回答">
                                <br>
                            </div>
                            <div class="col-md-8 col-md-offset-0">
                                <strong>
                                    <h4>2. 單選題</h4>
                                </strong>
                                <div class="form-group">
                                    <div class="radio radio-primary">
                                        <label>
                                        <input type="radio" name="option" value="A" id="radio1">
                                        選項A
                                        </label>
                                    </div>
                                    <div class="radio radio-primary">
                                        <label>
                                        <input type="radio" name="option" value="B" id="radio2">
                                        選項B
                                        </label>
                                    </div>
                                    <div class="radio radio-primary">
                                        <label>
                                        <input type="radio" name="option" value="C" id="radio3">
                                        選項C
                                        </label>
                                    </div>
                                    <div class="radio radio-primary">
                                        <label>
                                        <input type="radio" name="option" value="D" id="radio4">
                                        選項D
                                        </label>
                                    </div>
                                </div>
                                <div class="radio radio-primary">
                                    <label>
                                    <input type="radio" name="jizz" value="D" id="radio4">
                                    選項D
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-8 col-md-offset-0">
                                <strong>
                                    <h4>3. 多選題</h4>
                                </strong>
                                <div class="form-group">
                                    <div class="checkbox">
                                        <label id="check">
                                            <input type="checkbox" style="padding-right:5px">
                                            <span id="check-item">選項A</span>
                                        </label>
                                    </div>
                                    <div class="checkbox">
                                        <label id="check">
                                            <input type="checkbox">
                                            <span id="check-item">選項B</span>
                                        </label>
                                    </div>
                                    <div class="checkbox">
                                        <label id="check">
                                            <input type="checkbox">
                                            <span id="check-item">選項C</span>
                                        </label>
                                    </div>
                                    <div class="checkbox">
                                        <label id="check">
                                            <input type="checkbox">
                                            <span id="check-item">選項D</span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<%- include footer %>
