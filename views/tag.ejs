<%- include header  %>
	<section id="main">
	    <div class="container">
	        <div class="row">
	            <div class="col-md-9 ">
								<% var lastYear = 0; %>

									<table class="table table-striped table-hover ">
										<thead>
										  <tr>
										    <th>#</th>
												<th>Date</th>
										    <th>Title</th>
										  </tr>
										</thead>
										<tbody>
											<% posts.forEach(function(post, index){  %>

												<% if(lastYear != post.time.year) { %>
													<h3><%= post.time.year %></h3>
												<% lastYear = post.time.year } %>
											<tr class="success">
												<td><%= index+1 %></td>
												<td><time> <%= post.time.day %> </time></td>
												<td> <a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"> <%= post.title %> </a></td>
											</tr>
										</tbody>
										<% }) %>
									</table>



	            </div>
	        </div>
	    </div>
	</section>
<%- include footer  %>
