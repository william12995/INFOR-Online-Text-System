<%- include header %>
<div class="col-md-9 ">
      <% var lastYear = 0; %>
      <% posts.forEach(function(post, index){  %>

      <% if(lastYear != post.time.year) { %>
      <p>
         <strong><h5><%= post.time.year %></h5></strong>
      </p>
      <% lastYear = post.time.year } %>
      <div class="well">
      <p>
         <strong>Date<time>: <%= post.time.date %> </time></strong><br>
         <strong>Title: <a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"> <%= post.title %> </a></strong><br>
         <strong>Author: <%= post.name %></strong>
      </p>
   </div>
   <% }) %>
</div>
<%- include footer %>
