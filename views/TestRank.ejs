<%- include header  %>
	<section id="main">
	    <div class="container">
	        <div class="row">
	            <div class="col-md-9 ">
								<h3><%= title %> / 排名</h3>
									<table class="table table-striped">
										<thead>
										  <tr>
										    <th>#</th>
										    <th>名字</th>
											<th>答對數</th>
											<th>時間</th>
										  </tr>
										</thead>
										<tbody>
											<% userrank.forEach(function(doc, index){  %>
											<tr class="success">
												<th><%= index+1 %></th>
												<td> <a href="/user/<%= doc.username %>"> <%= doc.username %> </a></td>
												<td><%= doc.correct %> / <%= doc.testsum %></td>
												<td><%= doc.day %></td>
											</tr>
										</tbody>
										<% }) %>
									</table>



	            </div>
	        </div>
	    </div>
	</section>
<%- include footer  %>
