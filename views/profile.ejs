<%- include header %>
<section id="main">
   <div class="container">
      <div class="row">
         <div class="col-md-8 col-md-offset-2">
               <div class="row" style="padding-top:10%;">
                  <div class="col-md-4">
                     <img src="<%= user.head %>" alt="profile photo" class="img-circle" style="height:200px;width:200px">
                  </div>
                  <div class="col-md-6">
                     <h1><%= user.name %></h1>
                     <h4><%= user.email %></h4>
                  </div>
               </div>
         </div>
      </div>
   </div>
</section>
<%- include footer %>
