<%- include header  %>
	<section id="main">
	    <div class="container">
	        <div class="row">
	            <div class="col-md-9 ">
								<h3><%= title %></h3>
									<table class="table table-striped">
										<thead>
										  <tr>
										    <th>#</th>
										    <th>Title</th>
											<th>出題者</th>
										  </tr>
										</thead>
										<tbody>
											<% docs.forEach(function(doc, index){  %>
											<tr>
												<th><%= index+1 %></th>
												<td> <a href="/Ansform/<%= doc.name %>"> <%= doc.name %> </a></td>

												<td><a href="/user/<%= doc.username %>"> <%= doc.username %> </a></td>

												<td><a href="/rank/<%= doc.name %>"> 排行榜 </a></td>
											</tr>
										</tbody>
										<% }) %>
									</table>



	            </div>
	        </div>
	    </div>
	</section>
<%- include footer  %>
